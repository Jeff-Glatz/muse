<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.VBox?>
<?import org.controlsfx.control.ToggleSwitch?>
<VBox xmlns:fx="http://javafx.com/fxml"
      fx:controller="kungzhi.muse.ui.HeadbandConnectionController">
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
    </padding>
    <ToggleSwitch fx:id="clientToggle"
                  minWidth="120"
                  text="%label.text.osc-client"/>
    <ToggleSwitch fx:id="museIOToggle"
                  minWidth="120"
                  text="%label.text.muse-io"/>
</VBox>