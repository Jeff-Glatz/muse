<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>
<GridPane xmlns:fx="http://javafx.com/fxml"
          fx:controller="kungzhi.muse.ui.OscClientController">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="91.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="127.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints vgrow="SOMETIMES" minHeight="10.0" prefHeight="30.0"/>
    </rowConstraints>
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
    </padding>
    <Label text="%label.text.oscClient" GridPane.halignment="LEFT" GridPane.valignment="CENTER">
        <font>
            <Font size="11.0"/>
        </font>
    </Label>
    <ToggleButton fx:id="clientToggleButton"
                  GridPane.columnIndex="1"
                  mnemonicParsing="false"
                  onAction="#toggleClient"
                  text="off"/>
</GridPane>