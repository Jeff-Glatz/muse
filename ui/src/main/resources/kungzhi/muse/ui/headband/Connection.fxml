<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.VBox?>
<?import org.controlsfx.control.ToggleSwitch?>
<VBox xmlns:fx="http://javafx.com/fxml"
      fx:controller="kungzhi.muse.ui.headband.ConnectionController">
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
    </padding>
    <ToggleSwitch fx:id="clientToggle"
                  id="osc-client-toggle"
                  styleClass="service-control"
                  minWidth="218"
                  text="%label.text.osc-client">
        <padding>
            <Insets bottom="5.0"/>
        </padding>
    </ToggleSwitch>
    <ToggleSwitch fx:id="museIOToggle"
                  id="muse-io-toggle"
                  styleClass="service-control"
                  minWidth="218"
                  text="%label.text.muse-io"/>
</VBox>